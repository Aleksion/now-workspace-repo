{
   "name": "@test/now",
   "version": 2,
   "builds": [
      {
         "src": "packages/test/next.config.js",
         "use": "@now/next"
      }
   ],
   "routes": [
      {
         "src": "(.*)",
         "dest": "/packages/test$1",
         "continue": true
      },
      {
         "src": "/sitemap.xml",
         "dest": "/api/sitemap"
      },
      {
         "src": "^/favicon.ico",
         "dest": "/packages/test/static/favicon.ico"
      }
   ]
}